<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="214.5dp"
        android:layout_height="185dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logorosachacel" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="214.5dp"
        android:layout_height="185dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logomoodle" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="IES ROSA CHACEL"
        android:textColor="#E42F2F"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="120dp"
        android:layout_height="36dp"
        android:layout_marginTop="28dp"
        android:text="C/ de Abizanda, 70 Madrid"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="364dp"
        android:layout_height="269dp"
        android:layout_marginTop="16dp"
        android:text="Visita de profesoras del instituto Fos Nord de Munich Por tic.ies.rosachacel.madrid en Novedades El viernes 1 de Marzo recibimos la visita de varias profesoras de la Escuela Profesional Fos Nord de Munich, en el marco de una gira organizada por la Red Xarxa FP (https://www.xarxafp.org/) por institutos de Formación Profesional de Madrid. Las profesoras fueron acompañadas por Deina Fortes y María Jesús Cervero, coordinadoras de movilidad de los Ayuntamientos de Madrid y Munich respectivamente."
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.476"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <Button
        android:id="@+id/buttonAnterior"
        android:layout_width="129dp"
        android:layout_height="45dp"
        android:layout_marginBottom="72dp"
        android:background="#FFFFFF"
        android:backgroundTint="#FFFFFF"
        android:lineSpacingExtra="8sp"
        android:text="ANTERIOR"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        android:textStyle="bold"
        app:iconTint="#D53434"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/textView4" />

    <Button
        android:id="@+id/buttonSiguiente"
        android:layout_width="137dp"
        android:layout_height="45dp"
        android:layout_marginBottom="72dp"
        android:background="#FFFFFF"
        android:backgroundTint="#FFFFFF"
        android:lineSpacingExtra="8sp"
        android:text="SIGUIENTE"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        android:textStyle="bold"
        app:iconTint="#D53434"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/textView4" />
</androidx.constraintlayout.widget.ConstraintLayout>